annotations:
  helm.sh/images: |
    - image: dp.apps.rancher.io/containers/open-webui:0.6.18
      name: open-webui
  licenses: MIT
apiVersion: v2
appVersion: 0.6.18
dependencies:
- condition: ollama.enabled
  import-values:
  - child: service
    parent: ollama.service
  name: ollama
  repository: oci://dp.apps.rancher.io/charts
  version: 1.26.0
- condition: pipelines.enabled
  import-values:
  - child: service
    parent: pipelines.service
  name: pipelines
  repository: oci://dp.apps.rancher.io/charts
  version: 0.5.0
- alias: tika
  condition: tika.enabled
  name: apache-tika
  repository: oci://dp.apps.rancher.io/charts
  version: 3.2.2
- alias: redis-cluster
  condition: redis-cluster.enabled
  name: redis
  repository: oci://dp.apps.rancher.io/charts
  version: 2.3.0
description: Open WebUI is an extensible, feature-rich, and user-friendly self-hosted
  WebUI designed to operate entirely offline. It supports various LLM runners, including
  Ollama and OpenAI-compatible APIs.
home: https://apps.rancher.io/applications/open-webui
icon: https://raw.githubusercontent.com/open-webui/open-webui/main/static/favicon.png
keywords:
- llm
- chat
- web-ui
maintainers:
- name: SUSE LLC
  url: https://www.suse.com
name: open-webui
version: 7.2.0
